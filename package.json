{
  "name": "cu",
  "version": "2.1.0",
  "description": "Compute Unit - Secure sandboxed Lua compute environment in WebAssembly with persistent storage, arbitrary-precision arithmetic, zero-copy I/O, and fixed memory architecture",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "web/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "./build.sh",
    "demo": "cd demo && python3 -m http.server 8000",
    "test": "node --test tests/*.node.test.js",
    "test:browser": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright test && playwright show-report",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilson63/cu.git"
  },
  "keywords": [
    "compute-unit",
    "cu",
    "lua",
    "webassembly",
    "wasm",
    "compute",
    "sandbox",
    "persistent",
    "bigint",
    "arbitrary-precision",
    "zero-copy",
    "indexeddb",
    "zig",
    "financial-calculations",
    "ethereum",
    "web3"
  ],
  "author": "Tom Wilson <tom@hyper.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twilson63/cu/issues"
  },
  "homepage": "https://github.com/twilson63/cu#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0"
  },
  "dependencies": {
    "puppeteer": "^24.26.1"
  }
}
